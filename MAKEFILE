release: DRAWING.ASM VIDEO.OBJ RAND.OBJ KEYB.OBJ DRAWNUMB.OBJ
	ML /W3 /WX VIDEO.OBJ RAND.OBJ KEYB.OBJ DRAWNUMB.OBJ DRAWING.ASM /Fe DRAWING.EXE

debug: DRAWING.ASM VIDEOD.OBJ RANDD.OBJ KEYBD.OBJ DRAWNUMBD.OBJ
	ML /Zi /W3 /WX VIDEOD.OBJ RANDD.OBJ KEYBD.OBJ DRAWNUMBD.OBJ DRAWING.ASM /Fe DRAWINGD.EXE

VIDEO.OBJ: VIDEO.ASM
	ML /c /W3 /WX /FoVIDEO.OBJ /Ta VIDEO.ASM
	
VIDEOD.OBJ: VIDEO.ASM
	ML /c /Zi /W3 /WX /FoVIDEOD.OBJ /Ta VIDEO.ASM
	
RAND.OBJ: RAND.ASM
	ML /c /W3 /WX /FoRAND.OBJ /Ta RAND.ASM
	
RANDD.OBJ: RAND.ASM
	ML /c /Zi /W3 /WX /FoRANDD.OBJ /Ta RAND.ASM
	
KEYB.OBJ: KEYB.ASM
	ML /c /W3 /WX /FoKEYB.OBJ /Ta KEYB.ASM
	
KEYBD.OBJ: KEYB.ASM
	ML /c /Zi /W3 /WX /FoKEYBD.OBJ /Ta KEYB.ASM

DRAWNUMB.OBJ: DRAWNUMB.ASM
	ML /c /W3 /WX /FoDRAWNUMB.OBJ /Ta DRAWNUMB.ASM
	
DRAWNUMBD.OBJ: DRAWNUMB.ASM
	ML /c /Zi /W3 /WX /FoDRAWNUMBD.OBJ /Ta DRAWNUMB.ASM


clean:
	del *.EXE
	del *.OBJ

cv: debug
	cv /s /43 DRAWINGD.EXE
